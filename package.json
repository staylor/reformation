  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@babel/runtime": "7.8.3",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "core-js": "2.6.9",
    "debounce": "1.2.0",
    "decode-uri-component": "0.2.0",
    "draft-js": "0.11.4",
    "env-cmd": "8.0.2",
    "express": "4.17.1",
    "facepaint": "1.2.1",
    "forever": "0.15.3",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "html-escape": "^2.0.0",
    "http-proxy-middleware": "0.20.0",
    "invariant": "2.2.4",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.1",
    "kyt-runtime": "1.0.0-alpha.d758bd1c",
    "morgan": "1.9.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "polished": "3.4.2",
    "pretty-lights": "0.0.14",
    "prop-types": "15.7.2",
    "query-string": "6.10.1",
    "react": "16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "16.12.0",
    "react-helmet-async": "1.0.4",
    "react-router-dom": "5.1.2",
    "redraft": "0.10.2",
    "source-map-support": "0.5.16"
  },
  "scripts": {
    "test": "env-cmd dev BABEL_ENV=test kyt test -- --forceExit --detectOpenHandles",
    "test-watch": "env-cmd dev BABEL_ENV=test kyt test -- --watch --verbose false",
    "test-update": "BABEL_ENV=test kyt test -- -u",
    "dev": "env-cmd dev kyt dev -C kyt.dev.js",
    "prod:build": "NODE_ENV=production env-cmd prod kyt build -C kyt.prod.js",
    "prod:stop": "forever stop highforthis",
    "prod:start": "NODE_ENV=production env-cmd prod forever start -a forever.prod.json",
    "prod": "npm run prod:build && npm run prod:stop && npm run prod:start",
    "prod:local": "npm i && npm run prod:build && npm run prod:local:start",
    "prod:local:start": "env-cmd prod node build/server/main.js",
    "lint-fix": "eslint . --fix",
    "precommit": "eslint ."
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@types/react": "*",
    "babel-preset-kyt-react": "1.0.0-alpha.d758bd1c",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.3",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-pretty-lights": "0.0.20",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "1.3.1",
    "kyt": "1.0.0-alpha.d758bd1c",
    "raf": "3.4.1",
    "rimraf": "3.0.0"
  }
}
