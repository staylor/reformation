    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "debounce": "1.2.0",
    "decode-uri-component": "0.2.0",
    "draft-js": "0.11.4",
    "emotion": "9.2.12",
    "emotion-server": "9.2.12",
    "emotion-theming": "9.2.9",
    "env-cmd": "8.0.2",
    "express": "4.17.1",
    "facepaint": "1.2.1",
    "forever": "0.15.3",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "html-escape": "^2.0.0",
    "http-proxy-middleware": "0.19.1",
    "invariant": "2.2.4",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.1",
    "kyt-runtime": "1.0.0-beta.1",
    "morgan": "1.9.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "polished": "3.4.4",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-apollo": "2.4.1",
    "react-dom": "16.13.0",
    "react-emotion": "9.2.12",
    "react-helmet-async": "0.2.0",
    "react-router-dom": "5.1.2",
    "redraft": "0.10.2",
    "source-map-support": "^0.5.16",
    "styled-theming": "2.2.0"
  },
  "scripts": {
    "test": "env-cmd dev BABEL_ENV=test kyt test -- --forceExit --detectOpenHandles",
    "test-watch": "env-cmd dev BABEL_ENV=test kyt test -- --watch --verbose false",
    "test-update": "BABEL_ENV=test kyt test -- -u",
    "dev": "env-cmd dev kyt dev -C kyt.dev.js",
    "prod:build": "NODE_ENV=production env-cmd prod kyt build -C kyt.prod.js",
    "prod:stop": "forever stop highforthis",
    "prod:start": "NODE_ENV=production env-cmd prod forever start -a forever.prod.json",
    "prod": "npm run prod:build && npm run prod:stop && npm run prod:start",
    "prod:local": "yarn install && yarn prod:build && yarn prod:local:start",
    "prod:local:start": "env-cmd prod node build/server/main.js",
    "lint-fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "precommit": "eslint ."
    }
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-flow": "7.8.3",
    "babel-plugin-emotion": "9.2.11",
    "babel-preset-kyt-react": "1.0.0-beta.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint-import-resolver-webpack": "^0.12.1",
    "flow-bin": "0.119.1",
    "husky": "1.3.1",
    "jest-emotion": "9.2.11",
    "kyt": "1.0.0-beta.1",
    "raf": "3.4.1",
    "rimraf": "3.0.2"
  }
}
