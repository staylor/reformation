    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-preset-kyt-enzyme": "1.0.3",
    "jest-preset-kyt-styled": "1.0.4",
    "kyt": "1.0.0-beta.40.13",
    "prettier": "2.1.2",
    "rimraf": "3.0.2"
