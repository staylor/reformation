    "redraft": "0.10.2",
    "regenerator-runtime": "0.13.7",
    "source-map-support": "0.5.19"
  },
  "scripts": {
    "test": "env-cmd dev BABEL_ENV=test jest --forceExit --detectOpenHandles",
    "test-watch": "env-cmd dev BABEL_ENV=test jest --watch --verbose false",
    "test-update": "BABEL_ENV=test jest -u",
    "dev": "env-cmd dev kyt dev -C kyt.dev.js",
    "prod:build": "NODE_ENV=production env-cmd prod kyt -C kyt.prod.js build",
    "prod:stop": "forever stop highforthis",
    "prod:start": "NODE_ENV=production env-cmd prod forever start -a forever.prod.json",
    "prod": "npm run prod:build && npm run prod:stop && npm run prod:start",
    "prod:local": "yarn install && yarn prod:build && yarn prod:local:start",
    "prod:local:start": "env-cmd prod node build/server/main.js",
    "lint-fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "precommit": "eslint ."
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "babel-eslint": "10.1.0",
    "babel-preset-kyt-react": "1.0.4",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-kyt": "1.0.7",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jest-formatting": "2.0.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-lodash": "7.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-pretty-lights": "0.0.22",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "jest-preset-kyt-enzyme": "1.0.2",
    "jest-preset-kyt-styled": "1.0.2",
    "kyt": "1.0.0-beta.40.11",
    "prettier": "2.0.5",
    "rimraf": "3.0.2"
  }
